  "author": "sera1mu <ifrea0281@gmail.com>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "build": "yarn run lint:editorconfig && yarn run fmt:check && yarn run lint && yarn run build:tsc",
    "build:tsc": "tsc",
    "fmt": "prettier --write 'src/**/*.ts'",
    "fmt:check": "prettier --check 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "lint:editorconfig": "editorconfig-checker -exclude '(?:LICENSE|dist)'"
  },
  "dependencies": {
    "discord.js": "^12.5.3",
    "log4js": "^6.3.0",
    "mariadb": "^2.5.3",
    "sqlstring": "^2.3.2",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/sqlstring": "^2.3.0",
    "@types/ws": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
